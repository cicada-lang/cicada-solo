import { Category } from "category/category.cic"

class Initial {
  cat: Category

  initial: cat.Object
  initial_morphism(x: cat.Object): cat.Morphism(initial, x)
  initial_morphism_unique(
    x: cat.Object,
    f: cat.Morphism(initial, x),
  ): Equal(cat.Morphism(initial, x), f, initial_morphism(x))
}

Initial
