import { Category } from "category/category.cic"

class Terminal {
  cat: Category

  terminal: cat.Object
  terminal_morphism(x: cat.Object): cat.Morphism(x, terminal)
  terminal_morphism_unique(
    x: cat.Object,
    f: cat.Morphism(x, terminal),
  ): Equal(cat.Morphism(x, terminal), f, terminal_morphism(x))
}
