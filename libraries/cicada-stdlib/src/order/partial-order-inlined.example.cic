import { PartialOrder } from "./partial-order.cic"
import { PreOrder } from "./pre-order.cic"

class PartialOrderInlined {
  Element: Type
  Under(Element, Element): Type

  reflexive(x: Element): Under(x, x)
  transitive(x: Element, y: Element, z: Element, Under(x, y), Under(y, z)): Under(x, z)
  antisymmetric(x: Element, y: Element, Under(x, y), Under(y, x)): Equal(Element, x, y)
}


the(Equal(Type, PartialOrder, PartialOrderInlined), refl)
