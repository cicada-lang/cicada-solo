import { PartialOrder } from "order/partial-order.cic"
import { PreOrder } from "order/pre-order.cic"

class PartialOrderInlined {
  Element: Type
  Under(Element, Element): Type

  reflexive(x: Element): Under(x, x)
  transitive(
    x y z: Element,
    Under(x, y),
    Under(y, z)
  ): Under(x, z)

  antisymmetric(
    x y: Element,
    Under(x, y),
    Under(y, x),
  ): Equal(Element, x, y)
}


the Equal(Type, PartialOrder, PartialOrderInlined)
same
