import { Category } from "./category.cic"

class Epimorphism {
  cat: Category
  dom: cat.Object
  cod: cat.Object
  morphism: cat.Morphism(dom, cod)

  cancel_left(
    x: cat.Object,
    f: cat.Morphism(cod, x),
    g: cat.Morphism(cod, x),
    Equal(
      cat.Morphism(dom, x),
      cat.compose(dom, cod, x)(morphism, f),
      cat.compose(dom, cod, x)(morphism, g)),
  ): Equal(cat.Morphism(cod, x), f, g)
}
