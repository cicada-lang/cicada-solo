function the_nat_type(_: Trivial): Type {
  return Nat
}

class ABC {
  a: Trivial = sole
  b: the_nat_type(a)
  c: String
}

abc: ABC(sole, 2) = {
  a: sole
  b: 2
  c: "c"
}

abc

abc.a
abc.b
abc.c


class ABCDE {
  a: Trivial
  b: the_nat_type(a)
  c: String = "c"
  d: String
  e: String
}

abcde: ABCDE(sole, 2, "d") = {
  a: sole
  b: 2
  c: "c" // NOTE "c" is not equal to "d"
  d: "d"
  e: "e"
}

abcde

abcde.a
abcde.b
abcde.c
abcde.d
abcde.e
