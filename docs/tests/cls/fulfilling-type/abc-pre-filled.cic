function the_nat_type(_: Trivial): Type {
  return Nat
}

class ABC {
  a: Trivial = sole
  b: the_nat_type(a)
  c: String
}

let abc: ABC(sole, 2) = {
  a: sole,
  b: 2,
  c: "c",
}

abc

abc.a
abc.b
abc.c


class ABCDE {
  a: Trivial
  b: the_nat_type(a)
  c: String = "c"
  d: String
  e: String
}

let abcde: ABCDE(sole, 2, "c") = {
  a: sole,
  b: 2,
  c: "c",
  d: "d",
  e: "e",
}

abcde

abcde.a
abcde.b
abcde.c
abcde.d
abcde.e
